{
  "name": "fancy-cars",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run build && npm run server",
    "start:watch": "cross-env NODE_ENV=development concurrently -r \"npm run build:watch\" \"npm run server:watch\"",
    "build:watch": "npm run clean && webpack --progress --watch --config tools/webpack.config.babel.js",
    "build": "npm run clean && webpack --config tools/webpack.config.babel.js",
    "server": "node dist/server/server.bundle.js",
    "mockServer": "webpack --config tools/webpack.config.mockserver.babel.js && node dist/mockServer/mockServer.bundle.js",
    "server:watch": "wait-on dist/server/server.bundle.js && nodemon --config tools/nodemon.json",
    "lint:fix": "./node_modules/.bin/eslint --fix client",
    "test:unit": "cross-env BABEL_ENV=mocha mocha \"./test/setup.js\" \"./client/**/*.test.js\" --compilers js:babel-register --require babel-polyfill --require ignore-styles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.2",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "ignore-styles": "^5.0.1",
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha-no-only": "0.0.6",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "jsdom": "^11.6.2",
    "mini-css-extract-plugin": "^0.2.0",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.2",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "wait-on": "^2.1.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  }
}
